{%- liquid
  render 'block-video'
-%}

{% schema %}
{
  "name": "t:labels.product",
  "settings": [],
  "blocks": [
    {
      "type": "block-video",
      "name": "t:labels.video",
      "settings": [
        {
          "type": "video_url",
          "id": "external_video",
          "label": "YouTube/Vimeo",
          "accept": ["youtube", "vimeo"],
          "default": "https://www.youtube.com/watch?v=9bZkp7q19f0"
        }
      ]
    }
  ]
}
{% endschema %}
