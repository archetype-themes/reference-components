{%- liquid
  for block in section.blocks
    case block.type
      when 'note'
        render 'block-cart-note', block: block
    endcase
  endfor
-%}

{% schema %}
{
  "name": "t:labels.cart",
  "settings": [],
  "blocks": [
    {
      "type": "note",
      "name": "Cart note",
      "limit": 1
    }
  ]
}
{% endschema %}
